ninja-grid(no-data-text="No communication log yet..." 
        grid-title="Communication Log" 
        doc-svc="vm.docSvc" 
        list="vm.docSvc.doc.commLog" 
        create-method="addCommLog" 
        update-method="" 
        delete-method="removeCommLog"
        add-toggle="vm.sideTab.commLog"
        fields="{date: {input: 'text', header: 'Date', order: 10, width: 20, type: 'date'}," + 
                " time: {input: 'text', header: 'Time', order: 15, width: 20, type: 'time'}," +
                " commType: {input: 'text', header: 'Type', order: 20, width: 10, type: 'string'}," +
                " employee: {input: 'text', header: 'Employee', order: 30, width: 10, type: 'string'}," +
                " description: {input: 'text', header: 'Description', order: 30, width: 60, type: 'string'}}")
